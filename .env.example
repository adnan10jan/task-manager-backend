# =============================================
# Task Manager - Environment Variables Template
# =============================================
# Copy this file to .env and replace values
# DO NOT commit .env to version control!

# Database Configuration
DB_URL=jdbc:postgresql://localhost:5432/taskapp
DB_USERNAME=taskuser
DB_PASSWORD=taskpass

# JWT Configuration (minimum 32 characters)
JWT_SECRET=taskapp_secret_key_123456789_taskapp

# Spring Profile
SPRING_PROFILES_ACTIVE=dev

# Docker PostgreSQL (if using docker-compose)
POSTGRES_DB=taskapp
POSTGRES_USER=postgres
POSTGRES_PASSWORD=asa@123

# Application Port
SERVER_PORT=8080
